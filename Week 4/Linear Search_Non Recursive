#include<stdio.h>
int main(){
    int a[100],i,key,j;
    printf("Number of elements: ");
    scanf("%d",&i);
    printf("The elements: \n");
    for(j=1;j<=i;j++){
        scanf("%d",&a[j]);
    }
    printf("Give key: ");
    scanf("%d",&key);
    for(j=0;j<i;j++){
        if(a[j] == key){
            printf("position = %d\n",j);
        }
    }
}

/*OUTPUT:

Number of elements: 5
The elements:
2
4
7
3
9
Give key: 4
position = 2

Process returned 0 (0x0)   execution time : 16.344 s
Press any key to continue.*/
